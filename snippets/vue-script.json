{
  "import": {
    "prefix": "import",
    "body": [
      "import { $1 } from '$2'"
    ],
    "description": "import {...} from ..."
  },
  "ref": {
    "prefix": "vRef",
    "body": [
      "const ref = ref('')"
    ],
    "description": "ref片段"
  },
  "setup": {
    "prefix": "setup",
    "body": [
      "setup($3,$4) {",
      "\t$0",
      "\treturn {",
      "\t\t$1",
      "\t}",
      "},"
    ],
    "description": "setup片段"
  },
  "defineProps": {
